        <CheckBoxPreference android:title="@string/pref_opensles_title" android:key="@string/pref_opensles_key" android:defaultValue="@string/pref_opensles_default" android:dialogTitle="@string/pref_opensles_dlg" />
        <CheckBoxPreference android:title="@string/pref_disable_built_in_aec_title" android:key="@string/pref_disable_built_in_aec_key" android:defaultValue="@string/pref_disable_built_in_aec_default" android:dialogTitle="@string/pref_disable_built_in_aec_dlg" />
        <CheckBoxPreference android:title="@string/pref_disable_built_in_agc_title" android:key="@string/pref_disable_built_in_agc_key" android:defaultValue="@string/pref_disable_built_in_agc_default" android:dialogTitle="@string/pref_disable_built_in_agc_dlg" />
        <CheckBoxPreference android:title="@string/pref_disable_built_in_ns_title" android:key="@string/pref_disable_built_in_ns_key" android:defaultValue="@string/pref_disable_built_in_ns_default" android:dialogTitle="@string/pref_disable_built_in_ns_dlg" />
        <CheckBoxPreference android:title="@string/pref_enable_level_control_title" android:key="@string/pref_enable_level_control_key" android:defaultValue="@string/pref_enable_level_control_default" />
        <ListPreference android:entries="@array/speakerphone" android:title="@string/pref_speakerphone_title" android:key="@string/pref_speakerphone_key" android:defaultValue="@string/pref_speakerphone_default" android:dialogTitle="@string/pref_speakerphone_dlg" android:entryValues="@array/speakerphoneValues" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/pref_datasettings_title" android:key="@string/pref_datasettings_key">
        <CheckBoxPreference android:title="@string/pref_enable_datachannel_title" android:key="@string/pref_enable_datachannel_key" android:defaultValue="@string/pref_enable_datachannel_default" />
        <CheckBoxPreference android:title="@string/pref_ordered_title" android:key="@string/pref_ordered_key" android:defaultValue="@string/pref_ordered_default" />
        <EditTextPreference android:title="@string/pref_data_protocol_title" android:key="@string/pref_data_protocol_key" android:defaultValue="@string/pref_data_protocol_default" android:dialogTitle="@string/pref_data_protocol_dlg" android:inputType="text" />
        <CheckBoxPreference android:title="@string/pref_negotiated_title" android:key="@string/pref_negotiated_key" android:defaultValue="@string/pref_negotiated_default" />
        <EditTextPreference android:title="@string/pref_max_retransmit_time_ms_title" android:key="@string/pref_max_retransmit_time_ms_key" android:defaultValue="@string/pref_max_retransmit_time_ms_default" android:dialogTitle="@string/pref_max_retransmit_time_ms_dlg" android:inputType="number" />
        <EditTextPreference android:title="@string/pref_max_retransmits_title" android:key="@string/pref_max_retransmits_key" android:defaultValue="@string/pref_max_retransmits_default" android:dialogTitle="@string/pref_max_retransmits_dlg" android:inputType="number" />
        <EditTextPreference android:title="@string/pref_data_id_title" android:key="@string/pref_data_id_key" android:defaultValue="@string/pref_data_id_default" android:dialogTitle="@string/pref_data_id_dlg" android:inputType="number" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/pref_miscsettings_title" android:key="@string/pref_miscsettings_key">
        <EditTextPreference android:title="@string/pref_room_server_url_title" android:key="@string/pref_room_server_url_key" android:defaultValue="@string/pref_room_server_url_default" android:dialogTitle="@string/pref_room_server_url_dlg" android:inputType="text" />
        <CheckBoxPreference android:title="@string/pref_displayhud_title" android:key="@string/pref_displayhud_key" android:defaultValue="@string/pref_displayhud_default" android:dialogTitle="@string/pref_displayhud_dlg" />
        <CheckBoxPreference android:title="@string/pref_tracing_title" android:key="@string/pref_tracing_key" android:defaultValue="@string/pref_tracing_default" android:dialogTitle="@string/pref_tracing_dlg" />
