<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/pref_videosettings_title" android:key="@string/pref_videosettings_key">
        <CheckBoxPreference android:title="@string/pref_videocall_title" android:key="@string/pref_videocall_key" android:defaultValue="@string/pref_videocall_default" android:dialogTitle="@string/pref_videocall_dlg" />
        <CheckBoxPreference android:title="@string/pref_screencapture_title" android:key="@string/pref_screencapture_key" android:defaultValue="@string/pref_screencapture_default" />
        <CheckBoxPreference android:title="@string/pref_camera2_title" android:key="@string/pref_camera2_key" android:defaultValue="@string/pref_camera2_default" />
        <ListPreference android:entries="@array/videoResolutions" android:title="@string/pref_resolution_title" android:key="@string/pref_resolution_key" android:defaultValue="@string/pref_resolution_default" android:dialogTitle="@string/pref_resolution_dlg" android:entryValues="@array/videoResolutionsValues" />
        <ListPreference android:entries="@array/cameraFps" android:title="@string/pref_fps_title" android:key="@string/pref_fps_key" android:defaultValue="@string/pref_fps_default" android:dialogTitle="@string/pref_fps_dlg" android:entryValues="@array/cameraFps" />
        <CheckBoxPreference android:title="@string/pref_capturequalityslider_title" android:key="@string/pref_capturequalityslider_key" android:defaultValue="@string/pref_capturequalityslider_default" android:dialogTitle="@string/pref_capturequalityslider_dlg" />
        <ListPreference android:entries="@array/startBitrate" android:title="@string/pref_maxvideobitrate_title" android:key="@string/pref_maxvideobitrate_key" android:defaultValue="@string/pref_maxvideobitrate_default" android:dialogTitle="@string/pref_maxvideobitrate_dlg" android:entryValues="@array/startBitrate" />
        <EditTextPreference android:title="@string/pref_maxvideobitratevalue_title" android:key="@string/pref_maxvideobitratevalue_key" android:defaultValue="@string/pref_maxvideobitratevalue_default" android:dialogTitle="@string/pref_maxvideobitratevalue_dlg" android:inputType="number" />
        <ListPreference android:entries="@array/videoCodecs" android:title="@string/pref_videocodec_title" android:key="@string/pref_videocodec_key" android:defaultValue="@string/pref_videocodec_default" android:dialogTitle="@string/pref_videocodec_dlg" android:entryValues="@array/videoCodecs" />
        <CheckBoxPreference android:title="@string/pref_hwcodec_title" android:key="@string/pref_hwcodec_key" android:defaultValue="@string/pref_hwcodec_default" android:dialogTitle="@string/pref_hwcodec_dlg" />
        <CheckBoxPreference android:title="@string/pref_capturetotexture_title" android:key="@string/pref_capturetotexture_key" android:defaultValue="@string/pref_capturetotexture_default" android:dialogTitle="@string/pref_capturetotexture_dlg" />
        <CheckBoxPreference android:title="@string/pref_flexfec_title" android:key="@string/pref_flexfec_key" android:defaultValue="@string/pref_flexfec_default" android:dialogTitle="@string/pref_flexfec_dlg" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/pref_audiosettings_title" android:key="@string/pref_audiosettings_key">
        <ListPreference android:entries="@array/startBitrate" android:title="@string/pref_startaudiobitrate_title" android:key="@string/pref_startaudiobitrate_key" android:defaultValue="@string/pref_startaudiobitrate_default" android:dialogTitle="@string/pref_startaudiobitrate_dlg" android:entryValues="@array/startBitrate" />
        <EditTextPreference android:title="@string/pref_startaudiobitratevalue_title" android:key="@string/pref_startaudiobitratevalue_key" android:defaultValue="@string/pref_startaudiobitratevalue_default" android:dialogTitle="@string/pref_startaudiobitratevalue_dlg" android:inputType="number" />
        <ListPreference android:entries="@array/audioCodecs" android:title="@string/pref_audiocodec_title" android:key="@string/pref_audiocodec_key" android:defaultValue="@string/pref_audiocodec_default" android:dialogTitle="@string/pref_audiocodec_dlg" android:entryValues="@array/audioCodecs" />
        <CheckBoxPreference android:title="@string/pref_noaudioprocessing_title" android:key="@string/pref_noaudioprocessing_key" android:defaultValue="@string/pref_noaudioprocessing_default" android:dialogTitle="@string/pref_noaudioprocessing_dlg" />
